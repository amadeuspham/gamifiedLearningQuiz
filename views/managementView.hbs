<head>
    <script type="text/javascript" src="/js/management.js"> </script>
</head>
<aside class="questionaires" id="questionaires"> 
    <div class="container">
        <div class="row">
            <input type="text" class="questionaires_search" placeholder="Search...">
            <button type="button" id="questionaires_add"> + </button>
        </div>
        <div class="row">
            <ul class="questionaires ul">
            {{#if this}}
                {{#each this}}
                    {{>questionaire_listing value=@index name=this.title questions=this.questions}}
                {{/each}}
            {{else}} 
                   <li> empty </li>
            {{/if}}
            </ul>
        </div>
    </div>
</aside>


<div class="editing_view" class="container">
    {{#if first_time}}
        <h1> First time</h1>
    {{else}}
        {{#if this.[1]}}
            {{>editView name=this.1.title questions=this.1.questions}}
        {{else}}
            {{>editView name="Questionaire"}}
        {{/if}}
    {{/if}} 
</div>
